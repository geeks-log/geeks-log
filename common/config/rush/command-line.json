{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/command-line.schema.json",
  "commands": [
    {
      "commandKind": "bulk",
      "name": "lint",
      "summary": "Lint each projects. (npm run lint)",
      "enableParallelism": true
    },
    {
      "commandKind": "bulk",
      "name": "test",
      "summary": "Test each projects. (npm run test)",
      "enableParallelism": true
    },
    {
      "commandKind": "global",
      "name": "deploy",
      "summary": "Deploy production projects. (e.g. services/)",
      "shellCommand": "node common/commands/deploy"
    }
  ],
  "parameters": [
  ]
}
